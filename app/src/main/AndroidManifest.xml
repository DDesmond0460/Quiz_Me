<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.company.quizme" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.QuizMe" >
        <activity android:name=".Score_page" />
        <activity android:name=".Quiz_page" />
        <activity
            android:name=".Forgot_Password"
            android:parentActivityName=".login_page"
            android:theme="@style/Theme.QuizMe" />
        <activity
            android:name=".Sign_Up_Page"
            android:parentActivityName=".login_page"
            android:theme="@style/Theme.QuizMe" />
        <activity android:name=".login_page" >
            android:theme="@style/Theme.login_page"
        </activity>
        <activity
            android:name=".Splash_screen"
            android:theme="@style/Theme.QuizMe_splash" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>